<?xml version="1.0" encoding="utf-8"?>
<Project>

  <PropertyGroup>
    <LangVersion>latest</LangVersion>

    <Year>$([System.DateTime]::Now.ToString('yyyy'))</Year>
    <VersionPrefix>0.0.12</VersionPrefix>
    <VersionSuffix>alpha</VersionSuffix>

    <!-- Passed to vvvv.exe when starting from within VS -->
    <PackageRepositories>$(MSBuildThisFileDirectory)..\..;C:\Users\elias\vvvv\vvvv\vvvv50;C:\Users\elias\vvvv\vvvv\public-vl</PackageRepositories>
    <VLVersion>2021.4.0-0299-g680dbe04b5</VLVersion>
